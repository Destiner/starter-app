{
  "name": "starter-app",
  "version": "1.0.0",
  "description": "Starter Vue app",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "vue",
    "starter"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/starter-app.git"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:prettier": "prettier -c \"src/**/*.{json,js,ts,vue}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\"",
    "typecheck": "npx tsc --noEmit && npx vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "3.2.47",
    "eslint": "8.37.0",
    "eslint-import-resolver-typescript": "3.5.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.11.0",
    "prettier": "2.8.3",
    "stylelint": "^15.6.0",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "33.0.0",
    "typescript": "5.0.3",
    "vite": "^3.0.4",
    "vue-tsc": "1.6.2"
  }
}
